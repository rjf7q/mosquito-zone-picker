RANDOM FOREST

source('C:/Users/Robert/Desktop/ROC.R')
source('C:/Users/Robert/Desktop/TestSet.R')
source('C:/Users/Robert/Desktop/SPM_Panel.R')
setwd("C:/Users/Robert/Desktop")

ZIKAData<-read.csv("zikaData.csv",header=TRUE,sep=";")
zikaData<-na.omit(ZIKAData)
summary(zikaData)

ZIKAData<-test.set(zikaData)
zikaData.train<-ZIKAData$train
zikaData.test<-ZIKAData$test

library(randomForest)
zikaData.rf=randomForest(as.factor(y)~.,data=zikaData.train,ntree=2,importance=T,proximity=T)

plot(zikaData.rf,main="Zika Data Random Forest")
MDSplot(zikaData.rf,factor(zikaData.train[,10]),main="MDS Plot Zika Data")
varImpPlot(zikaData.rf,main="Variable Importance for Zika Data RF")
importance(zikaData.rf)
zikaData.rf.pred=predict(zikaData.rf,newdata=zikaData.test,type="prob")




DECISION TREES (2 Methods)

source('C:/Users/Robert/Desktop/ROC.R')
source('C:/Users/Robert/Desktop/TestSet.R')
source('C:/Users/Robert/Desktop/SPM_Panel.R')
setwd("C:/Users/Robert/Desktop")

ZIKAData<-read.csv("zikaData.csv",header=TRUE,sep=";")
zikaData<-na.omit(ZIKAData)
summary(zikaData)

ZIKAData<-test.set(zikaData)
zikaData.train<-ZIKAData$train
zikaData.test<-ZIKAData$test

install.packages("party")
library(party)

zikaData.ctree<-ctree(y~.,data=zikaData.train)
zikaData.ctree
summary(zikaData.ctree)
plot(zikaData.ctree)

zikaData.ctree.pred<-predict(zikaData.ctree,newdata=zikaData.test,type="prob")
zikaData.ctree.pred
summary(zikaData.ctree.pred)
plot(zikaData.ctree.pred)

install.packages("mvpart")
library(mvpart)

zikaData.mvpart<-mvpart(y~.,data=zikaData.train,method="class")
zikaData.mvpart
summary(zikaData.mvpart)
plot(zikaData.mvpart)

zikaData.mvpart.pred<-predict(zikaData.mvpart,newdata=zikaData.test,type="prob")
zikaData.mvpart.pred
summary(zikaData.mvpart.pred)
plot(zikaData.mvpart.pred)
