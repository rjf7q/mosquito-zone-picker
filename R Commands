ZIKAData<-read.csv("zikaData.csv",header=TRUE,sep=";")
zikaData<-na.omit(ZIKAData)
summary(zikaData)

ZIKAData<-test.set(zikaData)
zikaData.train<-ZIKAData$train
zikaData.test<-ZIKAData$test

install.packages("MASS")
library(MASS)

zikaData.glm<-glm(y~.,data=zikaDataTrain,family=binomial)
zikaData.glm.fit<-predict(zikaData.glm,type="response")
score.table(zikaData.glm.fit,zikaDataTrain$y)

plot(zikaData.glm.fit,zikaDataTrain$y,pch=21,main="Zika Data GLM Training Set",xlab="Posterior",ylab="Actual")

zikaData.glm.pred<-predict(zikaData.glm,newdata=zikaDataTest,type="response")
score.table(zikaData.glm.pred,zikaDataTest$y)

plot(zikaData.glm.pred,zikaDataTest$y,pch=21,main="zika Data GLM Test Set",xlab="Posterior",ylab="Actual")
